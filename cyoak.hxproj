  <!-- Build options -->
  <build>
    <option directives="flixel=4.2.1&#xA;openfl=3.6.1&#xA;lime=2.9.1&#xA;HXCPP_QUIET&#xA;openfl-next&#xA;tools=2.9.1&#xA;no-compilation&#xA;native&#xA;lime-native&#xA;openfl-native&#xA;windows&#xA;desktop&#xA;display" />
